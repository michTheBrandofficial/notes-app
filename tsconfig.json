{
  "compilerOptions": {
    "paths": {
      "@styles/*": ["./styles/*"],
      "@components/*": ["./components/*"],
      "@assets/*": ["./assets/*"],
      "@hooks": ["./hooks/index"],
      "@utils/*": ["./utils/*"]
    },
    "resolveJsonModule": true,
    "baseUrl": ".",
    "jsx": "react-jsxdev",
    "jsxImportSource": "nixix/types",
    "module": "es2022",
    "target": "es2022",
    "strict": true,
    "types": ["nixix/router", "nixix/dom", "nixix/primitives", "nixix/hoc"],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  }
}
